<?php

/**
 * @file
 * Bulk export of default_mobile_tools_presets objects generated by Bulk export module.
 */

/**
 * Implements hook_default_mobile_tools_device_groups().
 */
function mobile_tools_default_mobile_tools_device_groups() {
  $device_groups = array();

    $device_group = new stdClass();
    $device_group->disabled = FALSE; /* Edit this to true to make a default device_group disabled initially */
    $device_group->api_version = 1;
    $device_group->dgid = 'mobile';
    $device_group->title = 'Mobile';
    $device_group->purl_modifier = 'mobile';
    $device_group->theme = 'garland';
    $device_group->themekey_id = 12;
    $device_group->description = 'Sample device group.';
    $device_group->detector = 'mobile_tools_browscap';
    $device_group->detection_settings = array(
      'mobile_tools_browscap' => array(
        'activation callback' => 'mobile_tools_browscap_activate',
        'mobile_tools_browscap_parents' => array(
          'Android Browser 3.0' => 'Android Browser 3.0',
          'Android Browser 4.0' => 'Android Browser 4.0',
          'BlackBerry' => 'BlackBerry',
          'IEMobile' => 'IEMobile',
          'Mobile Safari 3.0' => 'Mobile Safari 3.0',
          'Mobile Safari 3.1' => 'Mobile Safari 3.1',
          'Mobile Safari 4.0' => 'Mobile Safari 4.0',
          'Mobile Safari 5.0' => 'Mobile Safari 5.0',
          'Mobile Safari 5.1' => 'Mobile Safari 5.1',
          'Mobile Safari 6.0' => 'Mobile Safari 6.0',
          'Palm Web' => 'Palm Web',
        ),
        'mobile_tools_browscap_user_agents' => array(),
      ),
    );
    $device_group->weight = 0;
    $device_group->httpheaders = array();
    $device_groups['mobile'] = $device_group;

    return $device_groups;
}